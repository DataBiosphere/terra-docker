{
    "storage_api": "https://storage.googleapis.com",
    "tools": [
        "python",
        "r"
    ],
    "version_master_file": "terra-docker-versions.json",
    "doc_suffix": "versions.json",
    "gcr_image_repo": "us.gcr.io/broad-dsp-gcr-public",
    "doc_bucket": "gs://terra-docker-image-documentation",
    "doc_bucket_no_prefix": "terra-docker-image-documentation",
    "image_data": {
        "terra-jupyter-bioconductor": {
            "base_label": "Bioconductor",
            "tools": ["python", "r"],
            "packages": ["BiocVersion", "tidyverse"],
            "version": "0.0.3",
            "automated_flags": {
                "generate_docs": true,
                "build": true
            }
        },
        "terra-jupyter-hail": {
            "base_label": "Hail",
            "tools": ["python"],
            "packages": ["hail"],
            "version": "0.0.2",
            "automated_flags": {
                "generate_docs": false,
                "build": false
            }
        },
        "terra-jupyter-python": {
            "base_label": "Python",
            "tools": ["python"],
            "packages": ["pandas", "scikit-learn"],
            "version": "0.0.3",
            "automated_flags": {
                "generate_docs": true,
                "build": true
            }
        },
        "terra-jupyter-base": {
            "base_label": "Minimal",
            "tools": ["python"],
            "packages": [],
            "version": "0.0.4",
            "automated_flags": {
                "generate_docs": true,
                "build": true
            }
        },
        "terra-jupyter-r": {
            "base_label": "R",
            "tools": ["r"],
            "packages": [],
            "version": "0.0.4",
            "automated_flags": {
                "generate_docs": true,
                "build": true
            }
        },
        "terra-jupyter-gatk": {
            "base_label": "GATK",
            "tools": ["python", "gatk"],
            "packages": [],
            "version": "0.0.3",
            "automated_flags": {
                "generate_docs": false,
                "build": true
            }
        }
    }
}